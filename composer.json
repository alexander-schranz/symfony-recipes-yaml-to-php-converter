{
    "name": "schranz/symfony-recipes-yaml-to-php-converter",
    "description": "A wrapper around symplify/config-transformer used to update recipes and using easy coding standard to fix them. ",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symplify/config-transformer": "dev-main as 11.1.4",
        "symfony/finder": "^6.1",
        "symfony/process": "^6.1",
        "nunomaduro/termwind": "^1.13",
        "symplify/easy-coding-standard": "^11.1.3@dev",
        "symplify/coding-standard": "^11.1.3@dev"
    },
    "minimum-stability": "dev",
    "bin": [
        "yaml-to-php"
    ]
}
